{
    "activateModules": {
        "attachments": true,
        "downloadTempFiles": false,
        "hcube": false,
        "loadLocal": true,
        "logFile": true,
        "lstFile": true
    },
    "aggregateWidgets": true,
    "autoGenInputGraphs": true,
    "customCss": false,
    "datatable": {
        "class": "hover",
        "filter": "bottom",
        "options": {
            "pageLength": 15
        },
        "rownames": false
    },
    "defaultRendererOutput": "miroPivot",
    "defCompMode": "split",
    "excelIncludeEmptySheets": true,
    "excelIncludeMeta": true,
    "handsontable": {
        "colWidths": 200,
        "contextMenu": {
            "allowColEdit": true,
            "allowRowEdit": true,
            "enabled": true
        },
        "height": 700,
        "readonly": false,
        "stretchH": "none"
    },
    "layoutSettings": {
        "maxTabsExpandedInput": 5,
        "maxTabsExpandedOutput": 5,
        "maxTabsExpandedPivotComp": 5,
        "maxTabsExpandedSplitComp": 5,
        "maxTabsExpandedTabComp": 5
    },
    "overwriteAliases": {
        "_scalars_out": {
            "newAlias": "Output Scalars"
        },
        "berad": {
            "newAlias": "km break-even radius (one-way; copy of BE_radius)"
        },
        "flow": {
            "newAlias": "t/yr shipped"
        },
        "flowij": {
            "newAlias": "t/yr shipped (sum over modes)"
        },
        "miro_dist": {
            "newAlias": "km distance (copy of dist)"
        },
        "pprice": {
            "newAlias": "€/t chip price (copy of P_chip for charts)"
        },
        "sitegm": {
            "newAlias": "€/yr GM per plant"
        },
        "totalgm": {
            "newAlias": "€/yr total GM"
        },
        "uclane": {
            "newAlias": "€/t unit cost per lane (copy of UC_lane)"
        },
        "umlane": {
            "newAlias": "€/t unit margin per lane (copy of UM_lane)"
        }
    },
    "overwriteSheetOrder": {
        "input": "_widgets"
    },
    "parseLogForMiroLogSyntax": false,
    "pivotCompSettings": {
        "emptyUEL": "-",
        "fixedColumns": true,
        "hideEmptyCols": false,
        "hidePivotControls": false,
        "tableSummarySettings": {
            "colEnabled": false,
            "colSummaryFunction": "sum",
            "rowEnabled": false,
            "rowSummaryFunction": "sum"
        }
    },
    "roundingDecimals": 2,
    "storeLogFilesDuration": 7,
    "theme": "browser"
}
