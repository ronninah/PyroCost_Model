C o m p i l a t i o n


   1  *======================================================================*
   2  *  Pyrolysis Upstream: Chipping + Handling + Transport (Farm -> Plant) *
   3  *  KTBL-driven costs + LABOR adders (Option B only)                     *
   4  *======================================================================*
   5   
      This version:
      - Keeps only Option B (profit maximization).
      - Adds labor cost to chipping, handling, and transport with toggles.
      - Computes Supply(i) endogenously from area, wooded share, yield, losses, MC.
  12   
  13  *------------------*
  14  * Sets & data      *
  15  *------------------*
  16  Sets
  17      i   "farms (alley-width scenarios)" / i1*i3 /
  18      j   "plants"                         / j1*j2 /
  19      m   "modes"                          / tractor, truck / ;
  20  Alias (i,ii);
  21   
  22  Table dist(i,j)  "km"
  23             j1   j2
  24  i1         20   60
  25  i2         45   30
  26  i3         80   25 ;
  27   
  28  *------------------*
  29  * Supply calculation (simple)
  30  *------------------*
  31  * i1=24 m alleys, i2=48 m, i3=96 m  -> wooded share from ALMA geometry
  32  Parameter alphaWood(i) "Wooded share of farm area (fraction)";
  33  alphaWood('i1') = 0.187 ;
  34  alphaWood('i2') = 0.119 ;
  35  alphaWood('i3') = 0.062 ;
  36   
  37  Scalar
  38    A_ha        "Farm arable area (ha, same for all i)"           / 200 /
  39    Y_SR_DM     "SRC baseline yield (t DM/ha/yr)"                 / 16  /
  40    delta_edge  "Edge-effect uplift (fraction)"                   / 0.40/
  41    lambdaLoss  "Harvest/handling loss fraction"                  / 0.05/
  42    MC          "Moisture content of delivered chips (fraction)"  / 0.35/ ;
  43   
  44  Parameter Supply(i) "as-received t/yr (computed)";
  45  Supply(i) = (A_ha * alphaWood(i) * Y_SR_DM * (1 + delta_edge) * (1 - lambdaLoss))
  46              / max(1e-6, 1 - MC) ;
  47   
  48  display Supply;
  49   
  50  Scalar Backhaul "1=one-way, 2=round-trip" / 2 / ;
  51   
  52  *------------------------------*
  53  * KTBL machine inputs (no labor)
  54  *------------------------------*
  55  Scalar
  56    Tractor_eur_per_h      "€/h Standardtraktor 120 kW (KTBL)"          / 41.84 /
  57    PTOChipper_eur_per_h   "€/h Holzhacker PTO 25 m3/h (KTBL)"          / 22.63 /
  58    Body_Tractor_eur_per_t "€/t Häckselaufbau 22 m3 (KTBL)"             / 0.82  /
  59    SemiTrailer_eur_per_t  "€/t Sattelzugauflieger 34 t (KTBL)"         / 0.89  /
  60    Bucket_eur_per_t       "€/t Leichtgutschaufel 1.5 m3 (KTBL)"        / 0.39  /
  61    FrontLoader_eur_per_h  "€/h Traktorfrontlader (KTBL)"               / 8.68  /
  62    Tractor_speed_kmh      "km/h (tractor road speed)"                  / 40    /
  63    Chipper_m3_per_h       "m3/h (nameplate)"                           / 25    /;
  64   
  65  Scalar
  66    BulkDensity_t_per_m3   "t/m3 loose chips"                            / 0.30 /
  67    Handling_tph           "t/h sustained load+unload"                    / 20   /;
  68   
  69  *------------------------------*
  70  * Labor settings (toggles)
  71  *------------------------------*
  72  Scalar
  73    IncludeLabor           "1=include operator labor; 0=exclude"         / 1 /
  74    IncludeChipOp          "1=pay chipper operator in chipping"          / 1 /
  75    IncludeLoader          "1=pay loader operator in handling"           / 1 /
  76    IncludeDriver          "1=pay driver labor for transport"            / 1 /
  77    AddLaborToTruckTkm     "1=add driver labor to truck tkm; 0=skip"     / 1 / ;
  78   
  79  * Wages (Germany 2025 minimum)
  80  Scalar
  81    WageBase_eur_per_h     "EUR/h statutory minimum (from 2025-01-01)"   / 12.82 /
  82    OncostFrac             "employer on-costs fraction (social etc.)"    / 0.22  /
  83    Wage_eur_per_h         "EUR/h wage incl. on-costs" ;
  84  Wage_eur_per_h = WageBase_eur_per_h * (1 + OncostFrac);
  85   
  86  * Transport assumptions for truck if splitting machine vs labor
  87  Scalar
  88    Truck_speed_kmh        "km/h (typical regional haul)"                / 70 /
  89    PayloadTruck_t         "t payload (semi)"                            / 25 / ;
  90   
  91  *------------------------------*
  92  * Derived capacities
  93  *------------------------------*
  94  Scalar
  95    PayloadTractor_t "t payload for tractor plus 22 m3 chip box" ;
  96  PayloadTractor_t = 22 * BulkDensity_t_per_m3;
  97   
  98  *------------------------------*
  99  * Unit cost components
 100  *------------------------------*
 101  Scalar
 102    C_chip_eurt_mach   "EUR/t chipping (machine-only)"
 103    C_hand_eurt_mach   "EUR/t handling (machine-only)" ;
 104  C_chip_eurt_mach = (Tractor_eur_per_h + PTOChipper_eur_per_h)
 105                   / max(1e-6, Chipper_m3_per_h * BulkDensity_t_per_m3);
 106  C_hand_eurt_mach = Bucket_eur_per_t
 107                   + FrontLoader_eur_per_h / max(1e-6, Handling_tph);
 108   
 109  Scalar
 110    Labor_chip_hpt  "h per t for chipping"
 111    Labor_hand_hpt  "h per t for load plus unload" ;
 112  Labor_chip_hpt = 1 / max(1e-6, Chipper_m3_per_h * BulkDensity_t_per_m3);
 113  Labor_hand_hpt = 1 / max(1e-6, Handling_tph);
 114   
 115  Scalar
 116    C_chip_eurt_lab   "EUR/t chipping labor"
 117    C_hand_eurt_lab   "EUR/t handling labor" ;
 118  C_chip_eurt_lab = IncludeLabor * IncludeChipOp * Wage_eur_per_h * Labor_chip_hpt;
 119  C_hand_eurt_lab = IncludeLabor * IncludeLoader * Wage_eur_per_h * Labor_hand_hpt;
 120   
 121  Scalar
 122    C_chip_eurt   "EUR/t chipping (machine plus labor)"
 123    C_handle_eurt "EUR/t handling (machine plus labor)" ;
 124  C_chip_eurt   = C_chip_eurt_mach + C_chip_eurt_lab;
 125  C_handle_eurt = C_hand_eurt_mach + C_hand_eurt_lab;
 126   
 127  Parameter C_surcharge(m) "EUR/t per trip/body" ;
 128  C_surcharge('tractor') = Body_Tractor_eur_per_t ;
 129  C_surcharge('truck')   = SemiTrailer_eur_per_t  ;
 130   
 131  Parameter C_tkm(m) "EUR per t-km by mode" ;
 132  Scalar
 133    C_tkm_trac_mach  "EUR per t-km tractor machine"
 134    C_tkm_trac_lab   "EUR per t-km tractor driver labor"
 135    C_tkm_truck_mach "EUR per t-km truck machine (placeholder)"
 136    C_tkm_truck_lab  "EUR per t-km truck driver labor" ;
 137   
 138  C_tkm_trac_mach = Tractor_eur_per_h
 139                    / max(1e-6, Tractor_speed_kmh * PayloadTractor_t);
 140  C_tkm_trac_lab  = IncludeLabor * IncludeDriver * Wage_eur_per_h
 141                    / max(1e-6, Tractor_speed_kmh * PayloadTractor_t);
 142   
 143  C_tkm_truck_mach = 0.12 ;
 144  C_tkm_truck_lab  = IncludeLabor * IncludeDriver * AddLaborToTruckTkm
 145                    * Wage_eur_per_h / max(1e-6, Truck_speed_kmh * PayloadTruck_t);
 146   
 147  C_tkm('tractor') = C_tkm_trac_mach + C_tkm_trac_lab;
 148  C_tkm('truck')   = C_tkm_truck_mach + C_tkm_truck_lab;
 149   
 150  *------------------*
 151  * Economics
 152  *------------------*
 153  Parameter P_chip(j) "€/t price at plant (positive) or gate fee (negative)";
 154  P_chip('j1') = 25 ;
 155  P_chip('j2') = 20 ;
 156   
 157  Parameter DemandUB(j) "t/yr max accepted at plant";
 158  DemandUB(j) = +inf ;
 159   
 160  *------------------*
 161  * Variables
 162  *------------------*
 163  Positive Variables
 164      x(i,j,m)   "t/yr shipped from i to j by mode m" ;
 165   
 166  Variables
 167      Revenue     "€/yr"
 168      VarCost     "€/yr"
 169      GrossMargin "€/yr objective" ;
 170   
 171  *------------------*
 172  * Equations
 173  *------------------*
 174  Equations
 175      SupplyLim(i)    "sum x <= Supply(i)"
 176      DemandLim(j)    "sum x <= DemandUB(j)"
 177      RevenueDef      "Revenue definition"
 178      CostDef         "Cost definition (machine+labor)"
 179      ObjDef          "GrossMargin = Revenue - VarCost";
 180   
 181  SupplyLim(i)..   sum((j,m), x(i,j,m)) =l= Supply(i) ;
 182  DemandLim(j)..   sum((i,m), x(i,j,m)) =l= DemandUB(j) ;
 183   
 184  RevenueDef.. Revenue =e= sum((i,j,m), P_chip(j) * x(i,j,m));
 185   
 186  CostDef..
 187      VarCost =e=
 188          (C_chip_eurt + C_handle_eurt) * sum((i,j,m), x(i,j,m))
 189        + sum((i,j,m), (Backhaul * dist(i,j) * C_tkm(m) + C_surcharge(m)) * x(i,j,m));
 190   
 191  ObjDef..  GrossMargin =e= Revenue - VarCost ;
 192   
 193  *------------------*
 194  * Solve (Option B)
 195  *------------------*
 196  option limrow=0, limcol=0;
 197  Display A_ha, alphaWood, Y_SR_DM, delta_edge, lambdaLoss, MC, Supply,
 198          C_chip_eurt, C_handle_eurt, C_tkm, C_surcharge, Wage_eur_per_h;
 199   
 200  Model Upstream_B "Option B: Maximize profit (with labor toggles)"
 201  / SupplyLim, DemandLim, RevenueDef, CostDef, ObjDef /;
 202   
 203  solve Upstream_B using LP maximizing GrossMargin ;
 204   
 205  *==============================================================*
 206  * Post-solve analytics: location-aware gross margin            *
 207  *==============================================================*
 208  Parameter
 209    UC_lane(i,j,m)  "€/t unit cost per lane i->j,m"
 210    UR_j(j)         "€/t unit revenue at plant j"
 211    UM_lane(i,j,m)  "€/t unit margin per lane (UR - UC)"
 212    Xsol(i,j,m)     "t/yr shipped"
 213    Rev_lane(i,j,m) "€/yr revenue by lane"
 214    Cost_lane(i,j,m)"€/yr cost by lane"
 215    GM_lane(i,j,m)  "€/yr gross margin by lane"
 216    Rev_site(j)     "€/yr revenue at plant j"
 217    Cost_site(j)    "€/yr cost at plant j"
 218    GM_site(j)      "€/yr gross margin at plant j"
 219    GM_farm(i)      "€/yr gross margin by farm i"
 220    GM_mode(m)      "€/yr gross margin by mode m"
 221    GM_total        "€/yr total gross margin"
 222    BE_price(i,j,m) "€/t break-even price per lane"
 223    BE_radius(j,m)  "km one-way break-even distance (by mode at plant)";
 224   
 225  UC_lane(i,j,m) = (C_chip_eurt + C_handle_eurt)
 226                 + (Backhaul * dist(i,j) * C_tkm(m) + C_surcharge(m));
 227  UR_j(j)        = P_chip(j);
 228  UM_lane(i,j,m) = UR_j(j) - UC_lane(i,j,m);
 229   
 230  Xsol(i,j,m)      = x.l(i,j,m);
 231  Rev_lane(i,j,m)  = UR_j(j) * Xsol(i,j,m);
 232  Cost_lane(i,j,m) = UC_lane(i,j,m) * Xsol(i,j,m);
 233  GM_lane(i,j,m)   = Rev_lane(i,j,m) - Cost_lane(i,j,m);
 234   
 235  Rev_site(j) = sum((i,m), Rev_lane(i,j,m));
 236  Cost_site(j)= sum((i,m), Cost_lane(i,j,m));
 237  GM_site(j)  = Rev_site(j) - Cost_site(j);
 238   
 239  GM_farm(i)  = sum((j,m), GM_lane(i,j,m));
 240  GM_mode(m)  = sum((i,j), GM_lane(i,j,m));
 241  GM_total    = sum((i,j,m), GM_lane(i,j,m));
 242   
 243  BE_price(i,j,m) = UC_lane(i,j,m);
 244  BE_radius(j,m)  = max(0,
 245                        (UR_j(j) - (C_chip_eurt + C_handle_eurt) - C_surcharge(m))
 246                        / max(1e-6, Backhaul * C_tkm(m)) );
 247   
 248  Display
 249    UR_j, UC_lane, UM_lane,
 250    Rev_lane, Cost_lane, GM_lane,
 251    Rev_site, Cost_site, GM_site, GM_farm, GM_mode, GM_total,
 252    BE_price, BE_radius;
 253   
 254  put_utility 'log' / '--- Location-aware GM report generated (Option B, labor toggles).';
 255   
 256  *=== MIRO/Excel outputs ===*
 258  Parameter
 259    Flow(i,j,m)       "t/yr shipped"
 260    FlowIJ(i,j)       "t/yr shipped (sum over modes)"
 261    SiteGM(j)         "€/yr GM per plant"
 262    TotalGM           "€/yr total GM"
 263    MIRO_dist(i,j)    "km distance (copy of dist)"
 264    UClane(i,j,m)     "€/t unit cost per lane (copy of UC_lane)"
 265    UMlane(i,j,m)     "€/t unit margin per lane (copy of UM_lane)"
 266    BErad(j,m)        "km break-even radius (one-way; copy of BE_radius)"
 267    Pprice(j)         "€/t chip price (copy of P_chip for charts)"
 268  ;
 270   
 271  Flow(i,j,m)   = x.l(i,j,m);
 272  FlowIJ(i,j)   = sum(m, x.l(i,j,m));
 273  SiteGM(j)     = sum((i,m), P_chip(j)*x.l(i,j,m)
 274                   - ((C_chip_eurt + C_handle_eurt)
 275                      + (Backhaul*dist(i,j)*C_tkm(m) + C_surcharge(m)))*x.l(i,j,m));
 276  TotalGM       = sum(j, SiteGM(j));
 277   
 278  MIRO_dist(i,j) = dist(i,j);
 279  UClane(i,j,m)  = UC_lane(i,j,m);
 280  UMlane(i,j,m)  = UM_lane(i,j,m);
 281  BErad(j,m)     = BE_radius(j,m);
 282  Pprice(j)      = P_chip(j);
 283   
 284  execute_unload 'miro_out.gdx',
 285    i, j, m,
 286    Flow, FlowIJ, SiteGM, TotalGM,
 287    MIRO_dist, UClane, UMlane, BErad, Pprice ;
 288   
 289  execute_unload 'excel_out.gdx',
 290     i, j, m,
 291     dist, UC_lane, UR_j, UM_lane, BE_price, BE_radius,
 292     Rev_lane, Cost_lane, GM_lane, GM_farm, GM_site, GM_mode, GM_total ;
