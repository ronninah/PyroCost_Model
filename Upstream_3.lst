   5   
      This version:
      - Keeps only Option B (profit maximization).
      - Adds labor cost to chipping, handling, and transport with toggles.
      - Leaves exports & post-solve analytics unchanged in shape.
  12   
  13  *------------------*
  14  * Sets & data      *
  15  *------------------*
  16  Sets
  17      i   "farms"       / i1*i3 /
  18      j   "plants"      / j1*j2 /
  19      m   "modes"       / tractor, truck / ;
  20  Alias (i,ii);
  21   
  22  Table dist(i,j)  "km"
  23             j1   j2
  24  i1         20   60
  25  i2         45   30
  26  i3         80   25 ;
  27   
  28  Parameter Supply(i) "as-received t/yr";
  29  Supply('i1') = 1200 ;
  30  Supply('i2') =  900 ;
  31  Supply('i3') =  700 ;
  32   
  33  Scalar Backhaul "1=one-way, 2=round-trip" / 2 / ;
  34   
  35  *------------------------------*
  36  * KTBL machine inputs          *
  37  * (machine-only; no labor)     *
  38  *------------------------------*
  39  Scalar
  40    Tractor_eur_per_h      "€/h Standardtraktor 120 kW (KTBL)"          / 41.84 /
  41    PTOChipper_eur_per_h   "€/h Holzhacker PTO 25 m3/h (KTBL)"          / 22.63 /
  42    Body_Tractor_eur_per_t "€/t Häckselaufbau 22 m3 (KTBL)"             / 0.82  /
  43    SemiTrailer_eur_per_t  "€/t Sattelzugauflieger 34 t (KTBL)"         / 0.89  /
  44    Bucket_eur_per_t       "€/t Leichtgutschaufel 1.5 m3 (KTBL)"        / 0.39  /
  45    FrontLoader_eur_per_h  "€/h Traktorfrontlader (KTBL)"               / 8.68  /
  46    Tractor_speed_kmh      "km/h (tractor road speed)"                  / 40    /
  47    Chipper_m3_per_h       "m3/h (nameplate)"                           / 25    /;
  48   
  49  Scalar
  50    BulkDensity_t_per_m3   "t/m3 loose chips"                            / 0.30 /
  51    Handling_tph           "t/h sustained load+unload"                    / 20   /;
  52   
  53  *------------------------------*
  54  * Labor settings (toggles)     *
  55  *------------------------------*
  56  Scalar
  57    IncludeLabor           "1=include operator labor; 0=exclude"         / 1 /
  58    IncludeChipOp          "1=pay chipper operator in chipping"          / 1 /
  59    IncludeLoader          "1=pay loader operator in handling"           / 1 /
  60    IncludeDriver          "1=pay driver labor for transport"            / 1 /
  61    AddLaborToTruckTkm     "1=add driver labor to truck tkm; 0=skip"     / 1 / ;
  62   
  63  * Wages (Germany 2025 minimum)
  64  Scalar
  65    WageBase_eur_per_h     "EUR/h statutory minimum (from 2025-01-01)"   / 12.82 /
  66    OncostFrac             "employer on-costs fraction (social etc.)"    / 0.22  /
  67    Wage_eur_per_h         "EUR/h wage incl. on-costs" ;
  68  Wage_eur_per_h = WageBase_eur_per_h * (1 + OncostFrac);
  69   
  70  * Transport assumptions for truck if splitting machine vs labor
  71  Scalar
  72    Truck_speed_kmh        "km/h (typical regional haul)"                / 70 /
  73    PayloadTruck_t         "t payload (semi)"                            / 25 / ;
  74   
  75  *------------------------------*
  76  * Derived capacities           *
  77  *------------------------------*
  78  Scalar
  79    PayloadTractor_t "t payload for tractor plus 22 m3 chip box" ;
  80  PayloadTractor_t = 22 * BulkDensity_t_per_m3;
  81   
  82  *------------------------------*
  83  * Unit cost components         *
  84  *------------------------------*
  85  * Machine-only parts
  86  Scalar
  87    C_chip_eurt_mach   "EUR/t chipping (machine-only)"
  88    C_hand_eurt_mach   "EUR/t handling (machine-only)" ;
  89  C_chip_eurt_mach = (Tractor_eur_per_h + PTOChipper_eur_per_h)
  90                   / max(1e-6, Chipper_m3_per_h * BulkDensity_t_per_m3);
  91  C_hand_eurt_mach = Bucket_eur_per_t
  92                   + FrontLoader_eur_per_h / max(1e-6, Handling_tph);
  93   
  94  * Labor intensities (hours per ton)
  95  Scalar
  96    Labor_chip_hpt  "h per t for chipping"
  97    Labor_hand_hpt  "h per t for load plus unload" ;
  98  Labor_chip_hpt = 1 / max(1e-6, Chipper_m3_per_h * BulkDensity_t_per_m3);
  99  Labor_hand_hpt = 1 / max(1e-6, Handling_tph);
 100   
 101  * Labor adders (EUR per t)
 102  Scalar
 103    C_chip_eurt_lab   "EUR/t chipping labor"
 104    C_hand_eurt_lab   "EUR/t handling labor" ;
 105  C_chip_eurt_lab = IncludeLabor * IncludeChipOp * Wage_eur_per_h * Labor_chip_hpt;
 106  C_hand_eurt_lab = IncludeLabor * IncludeLoader * Wage_eur_per_h * Labor_hand_hpt;
 107   
 108  * Total chipping/handling (used in model)
 109  Scalar
 110    C_chip_eurt   "EUR/t chipping (machine plus labor)"
 111    C_handle_eurt "EUR/t handling (machine plus labor)" ;
 112  C_chip_eurt   = C_chip_eurt_mach + C_chip_eurt_lab;
 113  C_handle_eurt = C_hand_eurt_mach + C_hand_eurt_lab;
 114   
 115  * Distance-independent surcharges
 116  Parameter C_surcharge(m) "EUR/t per trip/body" ;
 117  C_surcharge('tractor') = Body_Tractor_eur_per_t ;
 118  C_surcharge('truck')   = SemiTrailer_eur_per_t  ;
 119   
 120  * Transport EUR per t-km: machine and labor parts
 121  Parameter C_tkm(m) "EUR per t-km by mode" ;
 122  Scalar
 123    C_tkm_trac_mach  "EUR per t-km tractor machine"
 124    C_tkm_trac_lab   "EUR per t-km tractor driver labor"
 125    C_tkm_truck_mach "EUR per t-km truck machine (placeholder)"
 126    C_tkm_truck_lab  "EUR per t-km truck driver labor" ;
 127   
 128  C_tkm_trac_mach = Tractor_eur_per_h
 129                    / max(1e-6, Tractor_speed_kmh * PayloadTractor_t);
 130  C_tkm_trac_lab  = IncludeLabor * IncludeDriver * Wage_eur_per_h
 131                    / max(1e-6, Tractor_speed_kmh * PayloadTractor_t);
 132   
 133  * Replace the placeholder when you have truck head EUR/h machine-only
 134  C_tkm_truck_mach = 0.12 ;
 135  C_tkm_truck_lab  = IncludeLabor * IncludeDriver * AddLaborToTruckTkm
 136                    * Wage_eur_per_h / max(1e-6, Truck_speed_kmh * PayloadTruck_t);
 137   
 138  C_tkm('tractor') = C_tkm_trac_mach + C_tkm_trac_lab;
 139  C_tkm('truck')   = C_tkm_truck_mach + C_tkm_truck_lab;
 140   
 141  *------------------*
 142  * Economics        *
 143  *------------------*
 144  Parameter P_chip(j) "€/t price at plant (positive) or gate fee (negative)";
 145  P_chip('j1') = 25 ;
 146  P_chip('j2') = 20 ;
 147   
 148  Parameter DemandUB(j) "t/yr max accepted at plant";
 149  DemandUB(j) = +inf ;
 150   
 151  *------------------*
 152  * Variables        *
 153  *------------------*
 154  Positive Variables
 155      x(i,j,m)   "t/yr shipped from i to j by mode m" ;
 156   
 157  Variables
 158      Revenue     "€/yr"
 159      VarCost     "€/yr"
 160      GrossMargin "€/yr objective" ;
 161   
 162  *------------------*
 163  * Equations        *
 164  *------------------*
 165  Equations
 166      SupplyLim(i)    "sum x <= Supply(i)"
 167      DemandLim(j)    "sum x <= DemandUB(j)"
 168      RevenueDef      "Revenue definition"
 169      CostDef         "Cost definition (machine+labor)"
 170      ObjDef          "GrossMargin = Revenue - VarCost";
 171   
 172  SupplyLim(i)..   sum((j,m), x(i,j,m)) =l= Supply(i) ;
 173  DemandLim(j)..   sum((i,m), x(i,j,m)) =l= DemandUB(j) ;
 174   
 175  RevenueDef.. Revenue =e= sum((i,j,m), P_chip(j) * x(i,j,m));
 176   
 177  CostDef..
 178      VarCost =e=
 179          (C_chip_eurt + C_handle_eurt) * sum((i,j,m), x(i,j,m))
 180        + sum((i,j,m), (Backhaul * dist(i,j) * C_tkm(m) + C_surcharge(m)) * x(i,j,m));
 181   
 182  ObjDef..  GrossMargin =e= Revenue - VarCost ;
 183   
 184  *------------------*
 185  * Solve (Option B) *
 186  *------------------*
 187  option limrow=0, limcol=0;
 188  Display C_chip_eurt, C_handle_eurt, C_tkm, C_surcharge, Wage_eur_per_h;
 189   
 190  Model Upstream_B "Option B: Maximize profit (with labor toggles)"
 191  / SupplyLim, DemandLim, RevenueDef, CostDef, ObjDef /;
 192   
 193  solve Upstream_B using LP maximizing GrossMargin ;
 194   
 195  *==============================================================*
 196  * Post-solve analytics: location-aware gross margin            *
 197  *==============================================================*
 198  Parameter
 199    UC_lane(i,j,m)  "€/t unit cost per lane i->j,m"
 200    UR_j(j)         "€/t unit revenue at plant j"
 201    UM_lane(i,j,m)  "€/t unit margin per lane (UR - UC)"
 202    Xsol(i,j,m)     "t/yr shipped"
 203    Rev_lane(i,j,m) "€/yr revenue by lane"
 204    Cost_lane(i,j,m)"€/yr cost by lane"
 205    GM_lane(i,j,m)  "€/yr gross margin by lane"
 206    Rev_site(j)     "€/yr revenue at plant j"
 207    Cost_site(j)    "€/yr cost at plant j"
 208    GM_site(j)      "€/yr gross margin at plant j"
 209    GM_farm(i)      "€/yr gross margin by farm i"
 210    GM_mode(m)      "€/yr gross margin by mode m"
 211    GM_total        "€/yr total gross margin"
 212    BE_price(i,j,m) "€/t break-even price per lane"
 213    BE_radius(j,m)  "km one-way break-even distance (by mode at plant)";
 214   
 215  UC_lane(i,j,m) = (C_chip_eurt + C_handle_eurt)
 216                 + (Backhaul * dist(i,j) * C_tkm(m) + C_surcharge(m));
 217  UR_j(j)        = P_chip(j);
 218  UM_lane(i,j,m) = UR_j(j) - UC_lane(i,j,m);
 219   
 220  Xsol(i,j,m)      = x.l(i,j,m);
 221  Rev_lane(i,j,m)  = UR_j(j) * Xsol(i,j,m);
 222  Cost_lane(i,j,m) = UC_lane(i,j,m) * Xsol(i,j,m);
 223  GM_lane(i,j,m)   = Rev_lane(i,j,m) - Cost_lane(i,j,m);
 224   
 225  Rev_site(j) = sum((i,m), Rev_lane(i,j,m));
 226  Cost_site(j)= sum((i,m), Cost_lane(i,j,m));
 227  GM_site(j)  = Rev_site(j) - Cost_site(j);
 228   
 229  GM_farm(i)  = sum((j,m), GM_lane(i,j,m));
 230  GM_mode(m)  = sum((i,j), GM_lane(i,j,m));
 231  GM_total    = sum((i,j,m), GM_lane(i,j,m));
 232   
 233  BE_price(i,j,m) = UC_lane(i,j,m);
 234  BE_radius(j,m)  = max(0,
 235                        (UR_j(j) - (C_chip_eurt + C_handle_eurt) - C_surcharge(m))
 236                        / max(1e-6, Backhaul * C_tkm(m)) );
 237   
 238  Display
 239    UR_j, UC_lane, UM_lane,
 240    Rev_lane, Cost_lane, GM_lane,
 241    Rev_site, Cost_site, GM_site, GM_farm, GM_mode, GM_total,
 242    BE_price, BE_radius;
 243   
 244  put_utility 'log' / '--- Location-aware GM report generated (Option B, labor toggles).';
 245   
 246  *=== MIRO/Excel outputs ===*
 248  Parameter
 249    Flow(i,j,m)       "t/yr shipped"
 250    FlowIJ(i,j)       "t/yr shipped (sum over modes)"
 251    SiteGM(j)         "€/yr GM per plant"
 252    TotalGM           "€/yr total GM"
 253    MIRO_dist(i,j)    "km distance (copy of dist)"
 254    UClane(i,j,m)     "€/t unit cost per lane (copy of UC_lane)"
 255    UMlane(i,j,m)     "€/t unit margin per lane (copy of UM_lane)"
 256    BErad(j,m)        "km break-even radius (one-way; copy of BE_radius)"
 257    Pprice(j)         "€/t chip price (copy of P_chip for charts)"
 258  ;
 260   
 261  Flow(i,j,m)   = x.l(i,j,m);
 262  FlowIJ(i,j)   = sum(m, x.l(i,j,m));
 263  SiteGM(j)     = sum((i,m), P_chip(j)*x.l(i,j,m)
 264                   - ((C_chip_eurt + C_handle_eurt)
 265                      + (Backhaul*dist(i,j)*C_tkm(m) + C_surcharge(m)))*x.l(i,j,m));
 266  TotalGM       = sum(j, SiteGM(j));
 267   
 268  MIRO_dist(i,j) = dist(i,j);
 269  UClane(i,j,m)  = UC_lane(i,j,m);
 270  UMlane(i,j,m)  = UM_lane(i,j,m);
 271  BErad(j,m)     = BE_radius(j,m);
 272  Pprice(j)      = P_chip(j);
 273   
 274  execute_unload 'miro_out.gdx',
 275    i, j, m,
 276    Flow, FlowIJ, SiteGM, TotalGM,
 277    MIRO_dist, UClane, UMlane, BErad, Pprice ;
 278   
 279  execute_unload 'excel_out.gdx',
 280     i, j, m,
 281     dist, UC_lane, UR_j, UM_lane, BE_price, BE_radius,
 282     Rev_lane, Cost_lane, GM_lane, GM_farm, GM_site, GM_mode, GM_total ;
