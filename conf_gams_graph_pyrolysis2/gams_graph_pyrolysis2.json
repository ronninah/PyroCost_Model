{
    "activateModules": {
        "attachments": true,
        "downloadTempFiles": false,
        "hcube": false,
        "loadLocal": true,
        "logFile": true,
        "lstFile": true
    },
    "aggregateWidgets": true,
    "autoGenInputGraphs": true,
    "customCss": false,
    "datatable": {
        "class": "display",
        "filter": "bottom",
        "options": {
            "pageLength": 15
        },
        "rownames": false
    },
    "defaultRendererOutput": "miroPivot",
    "defCompMode": "split",
    "excelIncludeEmptySheets": true,
    "excelIncludeMeta": true,
    "handsontable": {
        "colWidths": 200,
        "contextMenu": {
            "allowColEdit": true,
            "allowRowEdit": true,
            "enabled": true
        },
        "height": 700,
        "readonly": false,
        "stretchH": "none"
    },
    "layoutSettings": {
        "maxTabsExpandedInput": 5,
        "maxTabsExpandedOutput": 5,
        "maxTabsExpandedPivotComp": 5,
        "maxTabsExpandedSplitComp": 5,
        "maxTabsExpandedTabComp": 5
    },
    "overwriteAliases": {
        "_scalars": {
            "newAlias": "Scalars"
        },
        "c_tkm": {
            "newAlias": "c_tkm"
        },
        "curvedist": {
            "newAlias": "curvedist"
        },
        "curveprice": {
            "newAlias": "curveprice"
        },
        "d_active": {
            "newAlias": "d_active"
        },
        "fixed_plant": {
            "newAlias": "fixed_plant"
        },
        "labor_on": {
            "newAlias": "labor_on"
        },
        "p_biochar": {
            "newAlias": "p_biochar"
        },
        "profitmap": {
            "newAlias": "profitmap"
        },
        "q_char": {
            "newAlias": "q_char"
        },
        "q_ship": {
            "newAlias": "q_ship"
        },
        "rev_elec": {
            "newAlias": "rev_elec"
        },
        "surcharge": {
            "newAlias": "surcharge"
        },
        "v_labor": {
            "newAlias": "v_labor"
        },
        "v_plant": {
            "newAlias": "v_plant"
        },
        "v_up": {
            "newAlias": "v_up"
        }
    },
    "overwriteSheetOrder": {
        "input": [
            "_scalars",
            "_widgets"
        ]
    },
    "parseLogForMiroLogSyntax": false,
    "pivotCompSettings": {
        "emptyUEL": "-",
        "fixedColumns": true,
        "hideEmptyCols": false,
        "hidePivotControls": false,
        "tableSummarySettings": {
            "colEnabled": false,
            "colSummaryFunction": "sum",
            "rowEnabled": false,
            "rowSummaryFunction": "sum"
        }
    },
    "roundingDecimals": 2,
    "storeLogFilesDuration": 7,
    "theme": "browser"
}
